				<ProtectedString name="Source"><![CDATA[--[[
FolderTool, the plugin that lets you easily group selected parts into a folder and ungroup them

Early Alpha
]]
-- Services
local Selection = game:GetService("Selection")
local ChangeHistoryService = game:GetService("ChangeHistoryService")
local selectedObjects = Selection:Get()

-- Plugin variables
local toolbar = plugin:CreateToolbar("FolderTool")

local groupintofolder = toolbar:CreateButton("Group selected items into a folder", "This button groups all items that are currently selected into a folder.", "rbxassetid://7079868279")
local ungroupfromfolder = toolbar:CreateButton("Ungroup items in selected folder", "This button ungroups all items that are in the selected folder.", "rbxassetid://4458901886")

groupintofolder.Click:Connect(function()
	if #selectedObjects == 0 then
		print("No items are selected for FolderTool to group")
	else
		local parent = selectedObjects[1].Parent
		local folder = Instance.new("Folder")
		folder.Name = "NewFolder"
		folder.Parent = parent
		print(selectedObjects)
		for _, item in ipairs(selectedObjects) do
			item.Parent = folder
		end
		ChangeHistoryService:SetWaypoint("Grouped selected items into a folder")
		print("FolderTool successfully grouped the selected items into a folder")
	end
end)

ungroupfromfolder.Click:Connect(function()
	if #selectedObjects == 0 then
		print("Select a folder to ungroup")
	elseif #selectedObjects == 1 then do
			local desc = selectedObjects[1]:GetDescendants()
			for _, item in ipairs(desc) do
				item.Parent = Selection.Parent
			end
			selectedObjects[1]:Destroy()
			print("Successfully ungrouped all items inside folder.")
			ChangeHistoryService:SetWaypoint("Ungrouped items inside folder")
	end
	elseif #selectedObjects > 2 then do
	print("You can only ungroup 1 folder at a time!")
		end
		end
end)]]></ProtectedString>
